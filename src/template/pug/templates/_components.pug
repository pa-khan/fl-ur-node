include ../vars
doctype html
block vars
html(lang="ru")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
		title=title
	body
		.wrap
			link(rel="stylesheet" href="template/css/style.css" + version)

			main.main
				block content


			+script('field', 'components')
			+script('check', 'components')
			+script('select', 'components')
			+script('popup', 'components')
			script(src="template/js/common.js" + version)
